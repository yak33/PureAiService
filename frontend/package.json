{
  "name": "ai-service-frontend",
  "version": "1.0.0",
  "description": "AI 服务平台前端界面",
  "packageManager": "pnpm@10.17.0",
  "engines": {
    "node": ">=16",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "install:deps": "pnpm install",
    "clean": "pnpm store prune",
    "clean:modules": "node -e \"require('fs').rmSync('node_modules',{recursive:true,force:true});\"",
    "clean:lock": "node -e \"require('fs').rmSync('pnpm-lock.yaml',{force:true});\"",
    "reset": "pnpm run clean:modules && pnpm run clean:lock && pnpm install"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.6.0",
    "highlight.js": "^11.11.1",
    "marked": "^16.3.0",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "eslint": "^8.50.0",
    "eslint-plugin-vue": "^9.17.0",
    "vite": "^5.0.0"
  }
}